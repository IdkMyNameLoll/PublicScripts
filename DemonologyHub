	local function gotCoffeeCup()
		for _, d in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if d:IsA("Model") then
				if d:GetAttribute("ItemName") then
					if d:GetAttribute("ItemName") == "Coffee Cup" then
						return true
					end
				end
			end
		end

		return false
	end

