	local wraithCheckSuccess = false
	local ISACTUALLYWRAITH = false
	local wraithCheckingProgress = false

	TabCool:AddButton({
		Name = "Check if Wraith (will trigger all salt lines)",
		Callback = function()
			if wraithCheckingProgress == true then return end
			wraithCheckingProgress = true

			local AllSaltPiles = getSaltPiles()

			if wraithCheckSuccess == false then
				if #AllSaltPiles.DisturbedPiles <= 0 then
					if #AllSaltPiles.NormalPiles > 0 then
						OrionLib:MakeNotification({
							Name = "Wraith Check",
							Content = "Checking for Wraith..",
							Image = "rbxassetid://7733658504",
							Time = 3.5
						})

						local checkFunc = nil

						checkFunc = workspace.SaltPiles.ChildAdded:Connect(function(d)
							if d.Name == "DisturbedSaltLine" then
								wraithCheckSuccess = true
								checkFunc:Disconnect()
								checkFunc = nil

								OrionLib:MakeNotification({
									Name = "Wraith Check",
									Content = "Not Wraith!",
									Image = "rbxassetid://7733658504",
									Time = 3.5
								})
								wraithCheckingProgress = false
							end
						end)

						for _, d in pairs(AllSaltPiles.NormalPiles) do
							task.spawn(function()
								firetouchinterest(workspace.Ghost.Torso,d:WaitForChild("GhostTracker"),0)
								firetouchinterest(workspace.Ghost.Torso,d:WaitForChild("GhostTracker"),1)
								--d:WaitForChild("GhostTracker").Size = Vector3.new(2048,2048,2048)
								--d:WaitForChild("GhostTracker").Position = workspace:WaitForChild("Ghost").HumanoidRootPart.Position
							end)
						end

						task.delay(2.5,function()
							if checkFunc then
								checkFunc:Disconnect()
								checkFunc = nil

								if wraithCheckSuccess == true then
									OrionLib:MakeNotification({
										Name = "Wraith Check",
										Content = "Not Wraith!",
										Image = "rbxassetid://7733658504",
										Time = 3.5
									})
									wraithCheckingProgress = false
								else
									OrionLib:MakeNotification({
										Name = "Wraith Check",
										Content = "Wraith detected!",
										Image = "rbxassetid://7733658504",
										Time = 3.5
									})
									ISACTUALLYWRAITH = true
									wraithCheckingProgress = false
								end
							end
						end)
					else
						OrionLib:MakeNotification({
							Name = "Wraith Check",
							Content = "Please Place a Salt line!",
							Image = "rbxassetid://7733658504",
							Time = 3.5
						})
						wraithCheckingProgress = false
					end
				else
					OrionLib:MakeNotification({
						Name = "Wraith Check",
						Content = "Not Wraith!",
						Image = "rbxassetid://7733658504",
						Time = 3.5
					})
					wraithCheckingProgress = false
				end
			else
				if ISACTUALLYWRAITH then
					OrionLib:MakeNotification({
						Name = "Wraith Check",
						Content = "Wraith detected!",
						Image = "rbxassetid://7733658504",
						Time = 3.5
					})
					wraithCheckingProgress = false
				else
					OrionLib:MakeNotification({
						Name = "Wraith Check",
						Content = "Not Wraith!",
						Image = "rbxassetid://7733658504",
						Time = 3.5
					})
					wraithCheckingProgress = false
				end
			end
		end    
	})

	TabPlayer:AddToggle({
		Name = "Notify Chat Messages",
		Default = false,
		Callback = function(Value)
			if Value == false then
				for _, d in pairs(stuff.playerChatted) do
					d:Disconnect()
				end
			else
				for _, d in pairs(game.Players:GetChildren()) do
					task.spawn(function()
						if d == game.Players.LocalPlayer then
						else
							stuff.playerChatted[d.Name] = d.Chatted:Connect(function(msg)
								task.spawn(function()
									local topName = d.Name

									if workspace:WaitForChild("Ragdolls"):FindFirstChild(d.Name) then
										topName = topName.. " (dead)"

										if stuff.seechats == "alive" then
											return
										end
									else
										if stuff.seechats == "dead" then
											return
										end
									end

									OrionLib:MakeNotification({
										Name = topName.." said:",
										Content = msg,
										Image = "rbxassetid://124871982298256",
										Time = 10
									})

									if stuff.sayInchat2 == true then
										sayChatMessage(topName.." said: ".. msg)
									end
								end)
							end)
						end
					end)
				end
			end
		end    
	})

