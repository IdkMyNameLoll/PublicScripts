		Name = "Notify ghost nearby (only works when hunting)",
		Default = false,
		Callback = function(Value)
			stuff.ghostSeePlayerEnabled = Value

			if Value == true then
				while stuff.ghostSeePlayerEnabled do
					task.wait(1)

					task.spawn(function()
						local distanceGhost = (workspace:WaitForChild("Ghost"):WaitForChild("HumanoidRootPart").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude

						if distanceGhost <= stuff.ghostSeePlayerDistance and distanceGhost >= 5 and workspace:WaitForChild("Ghost"):GetAttribute("Hunting") then
							if workspace:WaitForChild("Ghost"):GetAttribute("Hunting") == false then return end

							OrionLib:MakeNotification({
								Name = "Ghost Warning",
								Content = "The ghost is "..tostring(math.floor(distanceGhost)).." studs away from you.",
								Image = "rbxassetid://7733658504",
								Time = 3.5
							})
						end
					end)
				end
			end
		end    
	})

	TabCool:AddToggle({
		Name = "Notify ghost hunt event",
		Default = false,
		Callback = function(Value)
			if Value == true then
				stuff.warningHunt = workspace:WaitForChild("Ghost"):GetAttributeChangedSignal("Hunting"):Connect(function()
					if workspace:WaitForChild("Ghost"):GetAttribute("Hunting") == true then
						OrionLib:MakeNotification({
							Name = "Hunt Warning",
