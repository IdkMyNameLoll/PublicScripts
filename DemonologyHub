	local function sayChatMessage(msg)
		game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("AskSpiritBoxFromUI"):FireServer(msg)
	end

	local function getAdrenalineBoostWithCooffee()
		game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("CoffeeBrewing"):WaitForChild("FillCup"):FireServer()
		game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("CoffeeBrewing"):WaitForChild("AddFlavor"):FireServer("Mocha")
		game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("CoffeeBrewing"):WaitForChild("AddFlavor"):FireServer("Strawberry")
		game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("CoffeeBrewing"):WaitForChild("AddFlavor"):FireServer("Caramel")
		game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DrinkCoffee"):FireServer()
	end

	local function returnGhostInfo()
		local Ghost = workspace:WaitForChild("Ghost")

		return {
			age = tostring(Ghost:GetAttribute("Age")),
			favroom = Ghost:GetAttribute("FavoriteRoom"),
			crntroom = Ghost:GetAttribute("CurrentRoom"),
			gender = Ghost:GetAttribute("Gender"),
			mdlName = Ghost:GetAttribute("VisualModel")
		}
	end

	function roundToDecimal(num, decimalPlaces)
		local multiplier = 10 ^ decimalPlaces
		return math.floor(num * multiplier + 0.5) / multiplier
	end

	workspace:WaitForChild("Ghost"):GetAttributeChangedSignal("LaserVisible"):Connect(function()
		local GetVal = workspace:WaitForChild("Ghost"):GetAttribute("LaserVisible")

		if GetVal == true then
			if workspace:WaitForChild("Ghost"):GetAttribute("Transparency") then
				if workspace:WaitForChild("Ghost"):GetAttribute("Transparency") < 1 then
					ghostLaserVisible = true
				end
			end
		end
	end)

	workspace:WaitForChild("Ghost"):GetAttributeChangedSignal("Transparency"):Connect(function()
		local GetVal = workspace:WaitForChild("Ghost"):GetAttribute("Transparency")

		if GetVal < 1 then
			if workspace:WaitForChild("Ghost"):GetAttribute("LaserVisible") then
				if workspace:WaitForChild("Ghost"):GetAttribute("LaserVisible") == true then
					ghostLaserVisible = true
				end
			end
		end
	end)

	TabCool:AddToggle({
		Name = "View Ghost",
		Default = false,
		Callback = function(Value)
			stuff.SeeGhost = Value

			if Value == true then
				while stuff.SeeGhost == true do
					task.wait()
					if stuff.SeeGhost == true then
						if workspace:WaitForChild("Ghost"):GetAttribute("VisualModel") == "Biter" and game.Players.LocalPlayer:GetAttribute("TrypophobiaSafe") == true then
							workspace:WaitForChild("Ghost"):WaitForChild("HumanoidRootPart"):WaitForChild("Cat"):WaitForChild("ImageLabel").ImageTransparency = 0.25
						else
							if workspace:WaitForChild("Ghost"):GetAttribute("Transparency") then
								if workspace:WaitForChild("Ghost"):GetAttribute("Transparency") <= 0.25 then
									for _, d in pairs(workspace:WaitForChild("Ghost").VisibleParts:GetDescendants()) do
										if d:IsA("BasePart") then
											d.Transparency = workspace:WaitForChild("Ghost"):GetAttribute("Transparency")
										elseif d:IsA("ParticleEmitter") or d:IsA("Light") then
											d.Enabled = true
										end
									end
								else
									for _, d in pairs(workspace:WaitForChild("Ghost").VisibleParts:GetDescendants()) do
										if d:IsA("BasePart") then
											d.Transparency = 0.25
										elseif d:IsA("ParticleEmitter") or d:IsA("Light") then
											d.Enabled = true
										end
									end
								end
							else
								for _, d in pairs(workspace:WaitForChild("Ghost").VisibleParts:GetDescendants()) do
									if d:IsA("BasePart") then
										d.Transparency = 0.25
									elseif d:IsA("ParticleEmitter") or d:IsA("Light") then
										d.Enabled = true
									end
								end
							end
						end
					else
						if workspace:WaitForChild("Ghost"):GetAttribute("VisualModel") == "Biter" and game.Players.LocalPlayer:GetAttribute("TrypophobiaSafe") == true then
							workspace:WaitForChild("Ghost"):WaitForChild("HumanoidRootPart"):WaitForChild("Cat"):WaitForChild("ImageLabel").ImageTransparency = workspace:WaitForChild("Ghost"):GetAttribute("Transparency")
						else
							for _, d in pairs(workspace:WaitForChild("Ghost").VisibleParts:GetDescendants()) do
								if d:IsA("BasePart") then
									d.Transparency = workspace:WaitForChild("Ghost"):GetAttribute("Transparency")
								elseif d:IsA("ParticleEmitter") or d:IsA("Light") then
									d.Enabled = workspace:WaitForChild("Ghost"):GetAttribute("Transparency") < 1
								end
							end
						end
					end
				end
			else
				for _, d in pairs(workspace:WaitForChild("Ghost").VisibleParts:GetDescendants()) do
					if d:IsA("BasePart") then
						d.Transparency = 1
					elseif d:IsA("ParticleEmitter") or d:IsA("Light") then
						d.Enabled = false
					end
				end
			end
		end    
	})

	TabCool:AddToggle({
		Name = "View Ghost Handprints",
		Default = false,
		Callback = function(Value)
			if stuff.prints then
				stuff.prints:Disconnect()
				stuff.prints = nil
			end

			if Value == true then
				for _, d in pairs(workspace.Handprints:GetChildren()) do
					d:FindFirstChildOfClass("Decal").Transparency = 0
				end

				stuff.prints = workspace.Handprints.ChildAdded:Connect(function(handprint)
					handprint:FindFirstChildOfClass("Decal").Transparency = 0
				end)
			else
				if stuff.prints then
					stuff.prints:Disconnect()
					stuff.prints = nil
				end

				for _, d in pairs(workspace.Handprints:GetChildren()) do
					d:FindFirstChildOfClass("Decal").Transparency = 1
				end
			end
		end    
	})

