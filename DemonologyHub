	local looppppppNOTOOTO = nil
	local debouncenotifyghosttemp = false
	
	local function mentionCheckV1000(rrrrrr)
		if debouncenotifyghosttemp == false then
			debouncenotifyghosttemp = true

			OrionLib:MakeNotification({
				Name = "Room Temperature Check (Ghost)",
				Content = "The temperature of "..rrrrrr.." is under 0C!",
				Image = "rbxassetid://7733658504",
				Time = 3.5
			})
			
			task.wait(2.5)
			debouncenotifyghosttemp = false
		end
	end
	
	TabRooms:AddToggle({
		Name = "Notify Ghost Room temperature under 0C",
		Default = false,
		Callback = function(Value)
			if Value == false then
				if looppppppNOTOOTO then
					looppppppNOTOOTO:Disconnect()
					looppppppNOTOOTO = nil
				end
			else
				local room = returnGhostInfo().favroom

				if workspace:WaitForChild("Map"):WaitForChild("Rooms"):WaitForChild(room):GetAttribute("Temperature") <= 0 then
					mentionCheckV1000(room)
				end

				looppppppNOTOOTO = workspace:WaitForChild("Map"):WaitForChild("Rooms"):WaitForChild(room):GetAttributeChangedSignal("Temperature"):Connect(function()
					if workspace:WaitForChild("Map"):WaitForChild("Rooms"):WaitForChild(room):GetAttribute("Temperature") <= 0 then
						mentionCheckV1000(room)
					end
				end)
			end
		end    
	})
	
	TabRooms:AddToggle({
		Name = "Noclip Through Doors",
		Default = false,
		Callback = function(Value)
			for _, d in pairs(workspace.Doors:GetChildren()) do
				task.spawn(function()
					for _, d22 in pairs(d:WaitForChild("Door"):GetChildren()) do
						if d22:IsA("BasePart") and d22.Name ~= "GhostTracker" then
							d22.CanCollide = not Value
						end
					end
				end)
			end
		end    
	})
	
