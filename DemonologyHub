	TabCool:AddButton({
		Name = "Invite Everyone",
		Callback = function()
			if game.Players.LocalPlayer:GetAttribute("IsPartyLeader") == true then
				OrionLib:MakeNotification({
					Name = "Warning",
					Content = "Inviting everyone in the server..",
					Image = "rbxassetid://7733658504",
					Time = 5
				})

				for _, d in pairs(game.Players:GetChildren()) do
					if d ~= game.Players.LocalPlayer then
						if d:GetAttribute("TeamInvites") then
							if d:GetAttribute("TeamInvites") == "Everyone" then
								game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("InvitePlayerToParty"):FireServer(d)
								task.wait(0.65)
							elseif d:GetAttribute("TeamInvites") == "Friends" then
								if game.Players.LocalPlayer:IsFriendsWith(d.UserId) then
									game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("InvitePlayerToParty"):FireServer(d)
									task.wait(0.65)
								end
							end
						end
					end
				end

				OrionLib:MakeNotification({
					Name = "Warning",
					Content = "Done!",
					Image = "rbxassetid://7733658504",
					Time = 5
				})
			else
				OrionLib:MakeNotification({
					Name = "Warning",
					Content = "Youre not party leader!",
					Image = "rbxassetid://7733658504",
					Time = 5
				})
			end
		end    
	})
