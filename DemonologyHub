	TabTool:AddButton({
		Name = "Notify all rare items",
		Callback = function()
			local found = false

			for _, d in pairs(workspace:WaitForChild("Items"):GetChildren()) do
				if d:GetAttribute("ItemName") then
					local NameItem = d:GetAttribute("ItemName")
					local er = true

					if d:GetAttribute("Uninteractable") then
						if d:GetAttribute("Uninteractable") == true then
							er = false
						end
					end

					if d:GetAttribute("Broken") then
						if d:GetAttribute("Broken") == true then
							er = false
						end
					end

					if er then
						if NameItem == "Energy Drink" or NameItem == "Music Box" or NameItem == "Umbra Board" or NameItem == "Energy Watch" then
							found = true

							OrionLib:MakeNotification({
								Name = "Rare Item Warning",
								Content = "A Rare item "..NameItem.." has been found.",
								Image = "rbxassetid://7733658504",
								Time = 3.5
							})

							local hightlight = Instance.new("Highlight")
							hightlight.Parent = d
							hightlight.FillColor = Color3.fromRGB(37, 161, 255)
							game:GetService("Debris"):AddItem(hightlight,5)
						else
							if tonumber(d.Name) >= 100 then
								found = true

								OrionLib:MakeNotification({
									Name = "Rare Item Warning",
									Content = "A Rare item "..NameItem.." has been found.",
									Image = "rbxassetid://7733658504",
									Time = 3.5
								})

								local hightlight = Instance.new("Highlight")
								hightlight.Parent = d
								hightlight.FillColor = Color3.fromRGB(37, 161, 255)
								game:GetService("Debris"):AddItem(hightlight,5)
							end
						end
					end
				end
			end

			if not found then
				OrionLib:MakeNotification({
					Name = "Rare Item Warning",
					Content = "No rare items were found.",
					Image = "rbxassetid://7733658504",
					Time = 3.5
				})
			end
		end    
	})

